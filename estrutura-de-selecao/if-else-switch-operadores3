#include <stdio.h>
#include <stdlib.h>


int main ()
{
    float numero;
    printf("digite o numero\n\n");
    scanf("%f",&numero);
    if(numero>=20 && numero<=90){
     printf("o numero esta entre 20 e 90\n\n");
          }
    else {
     printf("o numero n esta entre 20 e 90\n\n");
         }
         
    system("PAUSE");
    return 0;
                
    }